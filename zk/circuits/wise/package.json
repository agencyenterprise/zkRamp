{
    "name": "@zkramp/circuits-circom-wise",
    "version": "0.0.1",
    "packageManager": "yarn@3.6.3",
    "description": "wise circuits for zkRamp",
    "main": "dist/generate_input.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "dependencies": {
        "@zk-email/circuits": "^2.0.1",
        "@zk-email/helpers": "^2.0.0",
        "buffer": "^6.0.3",
        "bullmq": "^5.1.5",
        "circomlib": "^2.0.5",
        "dotenv": "^16.4.1",
        "eml-format": "^0.6.1",
        "process": "^0.11.10",
        "snarkjs": "latest"
    },
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "worker:wise:start": "npx ts-node-dev worker.ts --respawn --transpile-only --no-notify --no-deps --prefer-ts-exts --watch ",
        "gen-input:wise:send": "npx tsx ../../package/generate_input.ts --email_file=./emls/wise_send.eml --payment_type=wise --circuit_type=send",
        "gen-input:wise:registration": "npx tsx ../../package/generate_input.ts --email_file=./emls/wise_send.eml --payment_type=wise --circuit_type=registration",
        "gen-input:wise:send:receive": "npx tsx ../../package/generate_input.ts --email_file=./emls/wise_receive.eml --payment_type=wise --circuit_type=send --output_file=input_wise_send_receive",
        "gen-input:wise:send:merchant": "npx tsx ../../package/generate_input.ts --email_file=./emls/wise_send_merchant.eml --payment_type=wise --circuit_type=send --output_file=input_wise_send_merchant",
        "gen-input:wise:registration:receive": "npx tsx ../../package/generate_input.ts --email_file=./emls/wise_receive.eml --payment_type=wise --circuit_type=registration --output_file=input_wise_registration_receive",
        "compile:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send cd ../../scripts && ./1_compile.sh && cd ..",
        "compile:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./1_compile.sh && cd ..",
        "genkey:non-chunked:unsafe:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send && cd ../../scripts && ./3_gen_zkey_unsafe.sh && cd ..",
        "genkey:non-chunked:unsafe:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./3_gen_zkey_unsafe.sh && cd ..",
        "uploadkeys:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send && cd ../../scripts && ./7_upload_to_s3.sh && cd ..",
        "uploadkeys:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./7_upload_to_s3.sh && cd ..",
        "test:wise:send": "yarn test ./test/wise_send.spec.ts",
        "test:wise:registration": "yarn test ./test/wise_registration.spec.ts",
        "genverifier:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send && cd ../../scripts && ./8_gen_sol_verifier.sh && cd ..",
        "genverifier:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./8_gen_sol_verifier.sh && cd ..",
        "gencalldata:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send && cd ../../scripts && ./9_gen_sol_calldata.sh && cd ..",
        "gencalldata:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./9_gen_sol_calldata.sh && cd ..",
        "genwitness:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send && cd ../../scripts && ./2_gen_wtns.sh && cd ..",
        "genwitness:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./2_gen_wtns.sh && cd ..",
        "genproof:wise:send": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_send && cd ../../scripts && ./6_gen_proof_rapidsnark.sh && cd ..",
        "genproof:wise:registration": "CIRCUIT_DIR=wise CIRCUIT_NAME=wise_registration && cd ../../scripts && ./6_gen_proof_rapidsnark.sh && cd .."
    },
    "devDependencies": {
        "@babel/preset-env": "^7.22.2",
        "@babel/preset-react": "^7.22.0",
        "@babel/preset-typescript": "^7.21.5",
        "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
        "@openzeppelin/contracts": "^4.9.1",
        "@types/atob": "^2.1.2",
        "@types/jest": "^29.5.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.0.6",
        "@types/node-forge": "^1.3.2",
        "@types/tar-stream": "^2.2.2",
        "babel-preset-jest": "^29.5.0",
        "browserify-fs": "^1.0.0",
        "browserstack-local": "^1.5.1",
        "browserstack-node-sdk": "^1.6.1",
        "chai": "^4.3.7",
        "circom_tester": "^0.0.19",
        "circomlibjs": "^0.1.7",
        "ganache": "^7.3.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-fetch-mock": "^3.0.3",
        "jest-junit": "^15.0.0",
        "madge": "^6.0.0",
        "mocha": "^10.2.0",
        "msw": "^1.0.1",
        "node-forge": "^1.3.1",
        "nodemon": "^2.0.19",
        "process": "^0.11.10",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "selenium-webdriver": "^4.8.1",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6",
        "util": "^0.12.5",
        "vite-plugin-commonjs": "^0.7.1",
        "vite-plugin-ngmi-polyfill": "^0.0.2",
        "vite-plugin-node-polyfills": "^0.8.2"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "testEnvironment": "node",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "transformIgnorePatterns": [
            "node_modules/(?!(@zk-email)/)"
        ]
    },
    "yarnPath": ".yarn/releases/yarn-3.6.3.cjs"
}
